# Streamlit Secrets Configuration Example
# 
# For Streamlit Cloud deployment with dataset from cloud storage:
# 1. Copy this file to .streamlit/secrets.toml (locally) or add to Streamlit Cloud Secrets
# 2. Replace the dataset_url with your actual cloud storage URL
#
# To add secrets in Streamlit Cloud:
# - Go to your app settings
# - Click "Secrets" in the sidebar
# - Add the key-value pairs below

[secrets]
# Example: AWS S3 URL (make sure the file is publicly accessible)
# dataset_url = "https://your-bucket.s3.amazonaws.com/application_train.csv"

# Example: Google Cloud Storage
# dataset_url = "https://storage.googleapis.com/your-bucket/application_train.csv"

# Example: Direct file URL
# dataset_url = "https://example.com/path/to/application_train.csv"
